{
    "name": "Folio",
    "displayName": "Folio",
    "description": "Ordered tab lists by folder",
    "publisher": "SenseLogic",
    "repository": "https://github.com/senselogic/FOLIO.git",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "Folio",
                    "name": "Tabs",
                    "when": "!searchView"
                }
            ]
        },
        "menus": {
        "view/item/context": [
            {
                "command": "extension.openInExplorer",
                "when": "view == Folio && viewItem == folder",
                "group": "navigation"
            },
            {
                "command": "extension.openInExplorer",
                "when": "view == Folio && viewItem == file",
                "group": "navigation"
            }
        ]
        },
        "commands": [
            {
                "command": "extension.openInExplorer",
                "title": "Open in Explorer"
            }
        ],
        "activationEvents": [
            "onView:Folio"
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "yarn run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.54.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3",
        "@vscode/test-electron": "^2.3.2"
    }
}
